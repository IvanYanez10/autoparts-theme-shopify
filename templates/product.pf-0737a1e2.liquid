            <div id="shopify-product-reviews" data-id="{{product.id}}">{{ product.metafields.spr.reviews }}</div>
          </div>
        </div>
      </div>
    </div>
    {% endif %}
    {% if settings.product_tab3 %}
    <div class="protabc-content">
      <p>{{ pages[settings.product_tab3_page].content }}</p>
    </div>
    {% endif %}               
